{
  "name": "my-native",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "eas build -p android",
    "prebuild": "rm -rf node_modules && npm install"
  },
  "dependencies": {
    "@expo/config-plugins": "9.0.0",
    "@expo/metro-runtime": "4.0.1",
    "@expo/vector-icons": "14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-firebase/app": "21.12.1",
    "@react-native-firebase/firestore": "21.12.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "7.0.14",
    "@react-navigation/stack": "7.1.1",
    "axios": "1.7.9",
    "expo": "~52.0.47",
    "expo-application": "~6.0.2",
    "expo-asset": "~11.0.5",
    "expo-constants": "~17.0.8",
    "expo-font": "^13.3.2",
    "expo-image-picker": "16.0.6",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "2.0.1",
    "expo-system-ui": "4.0.9",
    "expo-updates": "~0.27.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-country-picker-modal": "2.0.0",
    "react-native-dropdown-picker": "5.4.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-image-crop-picker": "0.42.0",
    "react-native-picker-select": "^9.3.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.4.0",
    "react-native-vector-icons": "10.2.0",
    "react-native-web": "0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-react": "^7.27.1",
    "eas-cli": "16.13.4",
    "metro": "^0.81.0",
    "metro-config": "0.79.1",
    "metro-resolver": "0.79.1"
  },
  "private": true,
  "resolutions": {
    "react": "18.2.0",
    "react-native": "0.72.7",
    "glob": "9.3.5",
    "rimraf": "4.4.1",
    "@babel/core": "7.23.2"
  },
  "expo": {
    "name": "my-native",
    "slug": "my-native",
    "runtimeVersion": "1.0.0",
    "sdkVersion": "52.0.0",
    "assetBundlePatterns": [
      "**/*.png",
      "**/*.jpg",
      "**/*.jpeg",
      "assets/**/*",
      "src/assets/**/*"
    ],
    "updates": {
      "url": "https://u.expo.dev/570fe10c-a217-4e2f-9f81-c7ca732de5ec"
    },
    "android": {
      "package": "com.medicalappointmentapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "../src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.medicalappointmentapp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos"
      }
    },
    "plugins": [
      "expo-image-picker",
      "expo-asset"
    ],
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-vector-icons",
          "firebase",
          "react-async-hook"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
